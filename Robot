public class Robot {

  private String[] geneticFunctions;
  private static final int genFunctNum = 5;
  private int generation;

  public Robot() {
    geneticFunctions = new String[genFunctNum];
    for (int i = 0; i < genFunctNum; i++) {
      geneticFunctions[i] = new ExpressionNode(0);
      geneticFunctions[i].grow();
    }
    generation = 0;
  }


}
